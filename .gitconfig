[user]
    email = kruanse@ya.ru
    name = KruASe76
[init]
    defaultBranch = main
[core]
    quotepath = false
    editor = nano
[safe]
    directory = *
[filter "lfs"]
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f
[alias]
    conf = config --global --edit
    confl = config --edit
    st = status -s
    sta = status
    a = add
    aa = add --all
    una = reset HEAD
    rsh = reset HEAD
    rs = reset
    cm = commit -m
    cam = commit -am
    amend = commit --amend
    ane = commit --amend --no-edit
    po = push origin
    pom = push origin main
    pfo = push --force-with-lease origin
    pfom = push --force-with-lease origin main
    fetcha = fetch --all --prune
    pullr = pull --rebase
    pulla = pull --all
    sw = switch
    co = checkout
    cod = checkout .
    discard = cod
    dc = discard
    br = branch -vv
    bra = branch -va
    brd = branch -d
    brdd = branch -D
    rb = rebase
    rbi = rebase --interactive
    rbc = rebase --continue
    rba = rebase --abort
    rmc = rm --cached
    graph = log --graph --oneline --decorate --all
    gr = log --graph --oneline --decorate
    gra = graph
    sh = stash
    shm = stash push -m
    sha = stash apply
    shp = stash pop
    shd = stash drop
    shl = stash list
    wta = worktree add
    wtr = worktree remove
    wtl = worktree list
[credential]
    helper = store
