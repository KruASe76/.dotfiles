[user]
    email = kruanse@ya.ru
    name = KruASe76
[init]
    defaultBranch = main
[core]
    quotepath = false
    editor = nano
[safe]
    directory = *
[filter "lfs"]
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
    clean = git-lfs clean -- %f
[alias]
    conf = config --global --edit
    st = status -s
    sta = status
    a = add
    aa = add --all
    una = reset HEAD
    rsh = reset HEAD
    rs = reset
    cm = commit -m
    cam = commit -am
    amend = commit --amend
    po = push origin
    pom = push origin main
    pfo = push --force-with-lease origin
    pfom = push --force-with-lease origin main
    pullr = pull --rebase
    sw = switch
    co = checkout
    cod = checkout .
    discard = cod
    dc = discard
    br = branch
    bra = branch -a
    brd = branch -d
    brdd = branch -D
    rb = rebase
    rbi = rebase --interactive
    rbc = rebase --continue
    rba = rebase --abort
    rmc = rm --cached
    graph = log --oneline --graph --decorate --all
    gr = graph
    stashm = stash push -m
    wta = worktree add
    wtr = worktree remove
    wtl = worktree list
