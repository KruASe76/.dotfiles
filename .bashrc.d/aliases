#!/bin/bash

alias su='sudo -i'
alias rm='rm -I --preserve-root'
alias mv='mv -i'
alias cp='cp -i'
alias ln='ln -i'
alias ll='ls -oh'
alias la='ls -oha'
alias c='clear'
alias up='sudo dnf update --refresh'
alias upy='sudo dnf -y update --refresh'
alias up-pip='pip --disable-pip-version-check list --outdated --format=json | python3 -c "import json, sys; print('"'"'\n'"'"'.join([x['"'"'name'"'"'] for x in json.load(sys.stdin)]))" | xargs -n1 pip install -U'
alias fix-power='sudo systemctl restart power-profiles-daemon.service'
alias meminfo='free -mlt'
alias bc='bc -l'
alias wget='wget -c'
alias open='xdg-open'
alias proton-run='env /home/kruase/PortWINE/PortProton/data/scripts/start.sh'
alias nv='nvidia-smi'
alias te='gnome-text-editor'
